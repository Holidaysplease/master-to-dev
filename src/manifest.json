{
    "manifest_version": 2,
    "name": "Master To Dev",
    "description": "",
    "version": "2.0",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Master to Dev",
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    }
}
